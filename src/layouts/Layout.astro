---
import Header from "../components/Header.astro";
import BaseHead from "../components/BaseHead.astro";

type Props = {
  title: string;
};

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <BaseHead title={title} />
  </head>
  <body>
    <Header />
    <main class="w-1/2 mx-auto my-7">
      <slot />
    </main>
  </body>
</html>
